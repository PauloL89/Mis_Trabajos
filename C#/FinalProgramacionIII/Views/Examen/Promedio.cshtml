@using FinalProgramacionIII.Models
@model List<Promedio>
@{
    ViewBag.Title = "Reporte";
}

<div class="row">
    <div class="col-sm-12">
        <div>
            <h4>Promedio</h4>
        </div>
    </div>
</div>


<div class="row">
    <div class="col-lg-6">
        <table class="table table-bordered table-responsive">
            <thead>
                <tr>
                    <th class="text-center"> Nivel</th>
                    <th class="text-center"> Promedio</th>


                </tr>
            </thead>
            <tbody>
                @foreach (var promedio in Model)
                {
                    <tr class=" text-center">

                        <td>
                            @promedio.nivel
                        </td>

                        <td>
                            @promedio.promedio
                        </td>

                    </tr>

                }
            </tbody>
        </table>
    </div>
</div>


